  pages = {73--94},
  file = {Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/479RCU6B/ees.2005.22.html:;CH4wetlands - Whalen.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/WX2S9KE4/CH4wetlands - Whalen.pdf:application/pdf}
