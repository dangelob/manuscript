@article{beer2010,
  pmid = {20603496},
  title = {Terrestrial Gross Carbon Dioxide Uptake: Global Distribution and Covariation with Climate},
  volume = {329},
  issn = {0036-8075, 1095-9203},
  shorttitle = {Terrestrial Gross Carbon Dioxide Uptake},
  doi = {10.1126/science.1184984},
  abstract = {Terrestrial gross primary production (GPP) is the largest global CO2 flux driving several ecosystem functions. We provide an observation-based estimate of this flux at 123 \ensuremath{\pm} 8 petagrams of carbon per year (Pg C year-1) using eddy covariance flux data and various diagnostic models. Tropical forests and savannahs account for 60\%. GPP over 40\% of the vegetated land is associated with precipitation. State-of-the-art process-oriented biosphere models used for climate predictions exhibit a large between-model variation of GPP's latitudinal patterns and show higher spatial correlations between GPP and precipitation, suggesting the existence of missing processes or feedback mechanisms which attenuate the vegetation response to climate. Our estimates of spatially distributed GPP and its covariation with climate can help improve coupled climate{\textendash}carbon cycle process models.},
  language = {en},
  timestamp = {2014-12-12 08:31:00},
  number = {5993},
  urldate = {2014-12-12},
  journal = {Science},
  author = {Beer, Christian and Reichstein, Markus and Tomelleri, Enrico and Ciais, Philippe and Jung, Martin and Carvalhais, Nuno and R{\"o}denbeck, Christian and Arain, M. Altaf and Baldocchi, Dennis and Bonan, Gordon B. and Bondeau, Alberte and Cescatti, Alessandro and Lasslop, Gitta and Lindroth, Anders and Lomas, Mark and Luyssaert, Sebastiaan and Margolis, Hank and Oleson, Keith W. and Roupsard, Olivier and Veenendaal, Elmar and Viovy, Nicolas and Williams, Christopher and Woodward, F. Ian and Papale, Dario},
  month = aug,
  year = {2010},
  keywords = {imprim{\'e},ipcc2013},
  pages = {834--838},
  file = {Beer et al. - 2010 - Terrestrial Gross Carbon Dioxide Uptake Global Di.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/8G6XNW5H/Beer et al. - 2010 - Terrestrial Gross Carbon Dioxide Uptake Global Di.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/DI7ZSE8F/834.html:text/html}
}

