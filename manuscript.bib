@book{farouki1981,
  series = {Series on rock and soil mechanics},
  title = {Thermal properties of soils},
  abstract = {Soil thermal properties are of importance in many engineering projects and other situations where heat transfer takes place in the soil. For example, they are of importance in the design of roads, airfields, pipelines or buildings in cold regions as well as underground power cables, hot water pipes or cold-gas pipelines in unfrozen ground. They are also important in such fields as agriculture, meteorology and geology. This book deals with the thermal properties of soils and the factors influencing them. It provides a framework in which these various factors and their effects may be placed. Topics covered include the following: mechanisms of heat transfer in soils; the effect of compositional, volumetric and other structural factors on the thermal properties of soils; the effect of water and its migration on the thermal properties of soils; further influence on the thermal properties of soils; and measurement of thermal properties of soils; and methods of calculating the thermal conductivity of soils},
