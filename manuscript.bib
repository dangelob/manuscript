  month = oct,
  year = {2013},
  keywords = {Carbon cycling,Eddy covariance,Inter-annual variations},
  pages = {675--682},
  file = {ScienceDirect Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/Z8G6CTQU/Gažovič et al. - 2013 - Hydrology-driven ecosystem respiration determines .pdf:application/pdf;ScienceDirect Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/FFEGDJ7V/S0048969713007298.html:text/html}
}

@article{silvola1996,
  title = {The contribution of plant roots to {CO2} fluxes from organic soils},
