@incollection{Luo2006161,
  address = {Burlington},
  title = {Chapter 8 - {Methods} of measurements and estimations},
  isbn = {978-0-12-088782-8},
  url = {http://www.sciencedirect.com/science/article/pii/B9780120887828500085},
  abstract = {Publisher Summary This chapter introduces a variety of methods for measurement of soil respiration. Accurate measurements of soil \{CO2\} efflux are extraordinarily challenging due to the properties of \{CO2\} transport in a porous medium of soil. Any measurement methods that disturb the soil \{CO2\} concentration and/or distort the gradient would result in serious errors. The most commonly used are chamber methods, which include the closed dynamic-chamber method, the open dynamic-chamber method, and the closed static-chamber method. Soil respiration is determined using alkali traps, by absorbing \{CO2\} released from the soil into a sealed headspace chamber for a specific period, using NaOH or \{KOH\} solutions. The soda-lime technique has been used for more than 40 years to measure \{CO2\} effluxes from soil under field conditions. Soil respiration can also be estimated from air samples from different depths of soil using the gas-well method. This chapter describes the basic principles behind those methods, discusses chamber designs and deployment, and assesses the accuracy and potential issues of those methods. It also briefly describes a few indirect methods for estimation of soil respiration.},
  timestamp = {2015-03-23 07:10:44},
  booktitle = {Soil {Respiration} and the {Environment}},
  publisher = {Academic {Press}},
  author = {Luo, {Yiqi} and {Zhou}, {Xuhui}},
  editor = {Luo, {Yiqi} and {Zhou}, {Xuhui}},
  year = {2006},
  pages = {161 -- 185}
}

