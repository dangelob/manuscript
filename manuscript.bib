@article{peichl2014,
  title = {{A} 12-year record reveals pre-growing season temperature and water table level threshold effects on the net carbon dioxide exchange in a boreal fen},
  volume = {9},
  issn = {1748-9326},
  doi = {10.1088/1748-9326/9/5/055006},
  abstract = {This study uses a 12-year time series (2001{\textendash}2012) of eddy covariance measurements to investigate the long-term net ecosystem exchange (NEE) of carbon dioxide (CO2) and inter-annual variations in relation to abiotic drivers in a boreal fen in northern Sweden. The peatland was a sink for atmospheric CO2 in each of the twelve study years with a 12-year average (\ensuremath{\pm} standard deviation) NEE of -58 \ensuremath{\pm} 21 g C m-2 yr-1. For ten out of twelve years, the cumulative annual NEE was within a range of -42 to -79 g C m-2 yr-1 suggesting a general state of resilience of NEE to moderate inter-annual climate variations. However, the annual NEE of -18 and -106 g C m-2 yr-1 in 2006 and 2008, respectively, diverged considerably from this common range. The lower annual CO2 uptake in 2006 was mainly due to late summer emissions related to an exceptional drop in water table level (WTL). A positive relationship (R 2 = 0.65) between pre-growing season (January to April) air temperature (Ta) and summer (June to July) gross ecosystem production (GEP) was observed. We suggest that enhanced GEP due to mild pre-growing season air temperature in combination with air temperature constraints on ecosystem respiration (ER) during the following cooler summer explained most of the greater net CO2 uptake in 2008. Differences in the annual and growing season means of other abiotic variables (e.g. radiation, vapor pressure deficit, precipitation) and growing season properties (i.e. start date, end date, length) were unable to explain the inter-annual variations of NEE. Overall, our findings suggest that this boreal fen acts as a persistent contemporary sink for atmospheric CO2 that is, however, susceptible to severe anomalies in WTL and pre-growing season air temperature associated with predicted changes in climate patterns for the boreal region.},
  language = {en},
  timestamp = {2015-05-28 14:54:05},
  number = {5},
  urldate = {2015-05-28},
  journal = {Environ. {Res}. {Lett}.},
  author = {Peichl, {Matthias} and {\"O}quist, {Mats} and {L}{\"o}fvenius, {Mikaell} {Ottosson} and {Ilstedt}, {Ulrik} and {Sagerfors}, {J}{\"o}rgen and {Grelle}, {Achim} and {Lindroth}, {Anders} and {Nilsson}, {Mats} {B}.},
  month = may,
  year = {2014},
  keywords = {imprimé},
  pages = {055006},
  file = {peichi.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/P7X9AKRZ/peichi.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/KX4KEXCA/055006.html:text/html}
}

@article{koehler2011,
  title = {How strong is the current carbon sequestration of an {Atlantic} blanket bog?},
  volume = {17},
  copyright = {{\textcopyright} 2010 {Blackwell} {Publishing} {Ltd}},
  issn = {1365-2486},
  doi = {10.1111/j.1365-2486.2010.02180.x},
  abstract = {Although northern peatlands cover only 3\% of the land surface, their thick peat deposits contain an estimated one-third of the world's soil organic carbon (SOC). Under a changing climate the potential of peatlands to continue sequestering carbon is unknown. This paper presents an analysis of 6 years of total carbon balance of an almost intact Atlantic blanket bog in Glencar, County Kerry, Ireland. The three components of the measured carbon balance were: the land-atmosphere fluxes of carbon dioxide (CO2) and methane (CH4) and the flux of dissolved organic carbon (DOC) exported in a stream draining the peatland. The 6 years C balance was computed from 6 years (2003{\textendash}2008) of measurements of meteorological and eddy-covariance CO2 fluxes, periodic chamber measurements of CH4 fluxes over 3.5 years, and 2 years of continuous DOC flux measurements. Over the 6 years, the mean annual carbon was -29.7\ensuremath{\pm}30.6 (\ensuremath{\pm}1 SD) g C m-2 yr-1 with its components as follows: carbon in CO2 was a sink of -47.8\ensuremath{\pm}30.0 g C m-2 yr-1; carbon in CH4 was a source of 4.1\ensuremath{\pm}0.5 g C m-2 yr-1 and the carbon exported as stream DOC was a source of 14.0\ensuremath{\pm}1.6 g C m-2 yr-1. For 2 out of the 6 years, the site was a source of carbon with the sum of CH4 and DOC flux exceeding the carbon sequestered as CO2. The average C balance for the 6 years corresponds to an average annual growth rate of the peatland surface of 1.3 mm yr-1.},
  language = {en},
  timestamp = {2015-09-08 11:20:41},
  number = {1},
  urldate = {2015-09-08},
  journal = {Glob. {Change} {Biol}.},
  author = {Koehler, {Ann}-{Kristin} and {Sottocornola}, {Matteo} and {Kiely}, {Gerard}},
  month = jan,
  year = {2011},
  keywords = {carbon balance,carbon sequestration,Dissolved organic carbon,Methane,Net ecosystem exchange,Peatland},
  pages = {309--319},
  file = {Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/8KEKXXCG/Koehler et al. - 2011 - How strong is the current carbon sequestration of .pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/XVAP94KV/abstract\;jsessionid=2E7DB0773E0822A9FE61BF41E6C2DA8B.html:text/html}
}

