@article{alm1997,
  title = {Reconstruction of the carbon balance for microsites in a boreal oligotrophic pine fen, {Finland}},
  volume = {110},
  doi = {10.1007/s004420050177},
  abstract = {Carbon dioxide (CO 2 ) exchange was studied at flark (minerotrophic hollow), lawn and hummock microsites in an oligotrophic boreal pine fen. Statistical response functions were constructed for the microsites in order to reconstruct the annual CO 2 exchange balance from climate data. Carbon accumulation was estimated from the annual net CO 2 exchange, methane (CH 4 ) emissions and leaching of carbon. Due to high water tables in the year 1993, the average carbon accumulation at the flark, Eriophorum lawn, Carex lawn and hummock microsites was high, 2.91, 6.08, 2.83 and 2.66 mol C m {\textendash}2 , respectively, and for the whole peatland it was 5.66 mol m {\textendash}2 year {\textendash}1 . During the maximum primary production period in midsummer, hummocks with low water tables emitted less methane than predicted from the average net ecosystem exchange (NEE), while the Carex lawns emitted slightly more. CH 4 release during that period corresponded to 16\% of the contemporary NEE. Annual C accumulation rate did not correlate with annual CH 4 release in the microsites studied, but the total community CO 2 release seemed to be related to CH 4 emissions in the wet microsites, again excluding the hummocks. The dependence of CO 2 exchange dynamics on weather events suggests that daily balances in C accumulation are labile and can change from net carbon uptake to net release, primarily in high hummocks on fens under warmer, drier climatic conditions.},
  timestamp = {2013-12-02 09:09:13},
  journal = {Oecologia},
  author = {Alm, {J}. and {Talanov}, {Alexander} and {Saarnio}, {Sanna} and {Silvola}, {Jouko} and {Ikkonen}, {Elena} and {Aaltonen}, {Heikki} and {Nyk}{\"a}nen, {Hannu} and {Martikainen}, {Pertti} {J}.},
  year = {1997},
  keywords = {accumulation,BdGaz,Carbon,CH4,Climate,Microsite,Peatland ecosystem},
  pages = {423--431},
  file = {Alm1997:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/SAVEEXMN/Alm1997.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/G99MJEG7/s004420050177.html:text/html}
}

