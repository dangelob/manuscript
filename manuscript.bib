@incollection{Luo2006161,
  address = {Burlington},
  title = {Chapter 8 - {Methods} of measurements and estimations},
  isbn = {978-0-12-088782-8},
  url = {http://www.sciencedirect.com/science/article/pii/B9780120887828500085},
  abstract = {Publisher Summary This chapter introduces a variety of methods for measurement of soil respiration. Accurate measurements of soil \{CO2\} efflux are extraordinarily challenging due to the properties of \{CO2\} transport in a porous medium of soil. Any measurement methods that disturb the soil \{CO2\} concentration and/or distort the gradient would result in serious errors. The most commonly used are chamber methods, which include the closed dynamic-chamber method, the open dynamic-chamber method, and the closed static-chamber method. Soil respiration is determined using alkali traps, by absorbing \{CO2\} released from the soil into a sealed headspace chamber for a specific period, using NaOH or \{KOH\} solutions. The soda-lime technique has been used for more than 40 years to measure \{CO2\} effluxes from soil under field conditions. Soil respiration can also be estimated from air samples from different depths of soil using the gas-well method. This chapter describes the basic principles behind those methods, discusses chamber designs and deployment, and assesses the accuracy and potential issues of those methods. It also briefly describes a few indirect methods for estimation of soil respiration.},
  timestamp = {2015-03-23T07:10:44Z},
  booktitle = {Soil {Respiration} and the {Environment}},
  publisher = {Academic {Press}},
  author = {Luo, {Yiqi} and {Zhou}, {Xuhui}},
  editor = {Luo, {Yiqi} and {Zhou}, {Xuhui}},
  year = {2006},
  pages = {161 -- 185}
}

@article{nilsson2008,
  title = {Contemporary carbon accumulation in a boreal oligotrophic minerogenic mire {\textendash} a significant sink after accounting for all {C}-fluxes},
  volume = {14},
  copyright = {{\textcopyright} 2008 {The} {Authors}. {Journal} compilation {\textcopyright} 2008 {Blackwell} {Publishing} {Ltd}},
  issn = {1365-2486},
  doi = {10.1111/j.1365-2486.2008.01654.x},
  abstract = {Based on theories of mire development and responses to a changing climate, the current role of mires as a net carbon sink has been questioned. A rigorous evaluation of the current net C-exchange in mires requires measurements of all relevant fluxes. Estimates of annual total carbon budgets in mires are still very limited. Here, we present a full carbon budget over 2 years for a boreal minerogenic oligotrophic mire in northern Sweden (64{\textdegree}11\ensuremath{'}N, 19{\textdegree}33\ensuremath{'}E). Data on the following fluxes were collected: land{\textendash}atmosphere CO2 exchange (continuous Eddy covariance measurements) and CH4 exchange (static chambers during the snow free period); TOC (total organic carbon) in precipitation; loss of TOC, dissolved inorganic carbon (DIC) and CH4 through stream water runoff (continuous discharge measurements and regular C-concentration measurements). The mire constituted a net sink of 27\ensuremath{\pm}3.4 (\ensuremath{\pm}SD) g C m-2 yr-1 during 2004 and 20\ensuremath{\pm}3.4 g C m-2 yr-1 during 2005. This could be partitioned into an annual surface{\textendash}atmosphere CO2 net uptake of 55\ensuremath{\pm}1.9 g C m-2 yr-1 during 2004 and 48\ensuremath{\pm}1.6 g C m-2 yr-1 during 2005. The annual NEE was further separated into a net uptake season, with an uptake of 92 g C m-2 yr-1 during 2004 and 86 g C m-2 yr-1 during 2005, and a net loss season with a loss of 37 g C m-2 yr-1 during 2004 and 38 g C m-2 yr-1 during 2005. Of the annual net CO2-C uptake, 37\% and 31\% was lost through runoff (with runoff TOC\ensuremath{>}DIC\ensuremath{\gg}CH4) and 16\% and 29\% through methane emission during 2004 and 2005, respectively. This mire is still a significant C-sink, with carbon accumulation rates comparable to the long-term Holocene C-accumulation, and higher than the C-accumulation during the late Holocene in the region.},
  language = {en},
  timestamp = {2015-08-19T05:14:15Z},
  number = {10},
  urldate = {2015-08-19},
  journal = {Glob. {Change} {Biol}.},
  author = {Nilsson, {Mats} and {Sagerfors}, {J}{\"o}rgen and {Buffam}, {Ishi} and {Laudon}, {Hjalmar} and {Eriksson}, {Tobias} and {Grelle}, {Achim} and {Klemedtsson}, {Leif} and {Weslien}, {Per} and {Lindroth}, {Anders}},
  month = oct,
  year = {2008},
  keywords = {boreal mire,carbon balance,DOC,Eddy covariance,Methane,NECB,NEE,Peat,Runoff,Sphagnum,TOC},
  pages = {2317--2332},
  file = {Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/E5PP3ZTF/Nilsson et al. - 2008 - Contemporary carbon accumulation in a boreal oligo.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/IKZAJEKE/abstract\;jsessionid=C99BB0557F7FA2A5FA3410AF4C6247B4.html:}
}

@article{alm1997,
  title = {Reconstruction of the carbon balance for microsites in a boreal oligotrophic pine fen, {Finland}},
  volume = {110},
  doi = {10.1007/s004420050177},
  abstract = {Carbon dioxide (CO 2 ) exchange was studied at flark (minerotrophic hollow), lawn and hummock microsites in an oligotrophic boreal pine fen. Statistical response functions were constructed for the microsites in order to reconstruct the annual CO 2 exchange balance from climate data. Carbon accumulation was estimated from the annual net CO 2 exchange, methane (CH 4 ) emissions and leaching of carbon. Due to high water tables in the year 1993, the average carbon accumulation at the flark, Eriophorum lawn, Carex lawn and hummock microsites was high, 2.91, 6.08, 2.83 and 2.66 mol C m {\textendash}2 , respectively, and for the whole peatland it was 5.66 mol m {\textendash}2 year {\textendash}1 . During the maximum primary production period in midsummer, hummocks with low water tables emitted less methane than predicted from the average net ecosystem exchange (NEE), while the Carex lawns emitted slightly more. CH 4 release during that period corresponded to 16\% of the contemporary NEE. Annual C accumulation rate did not correlate with annual CH 4 release in the microsites studied, but the total community CO 2 release seemed to be related to CH 4 emissions in the wet microsites, again excluding the hummocks. The dependence of CO 2 exchange dynamics on weather events suggests that daily balances in C accumulation are labile and can change from net carbon uptake to net release, primarily in high hummocks on fens under warmer, drier climatic conditions.},
  timestamp = {2013-12-02T09:09:13Z},
  journal = {Oecologia},
  author = {Alm, {J}. and {Talanov}, {Alexander} and {Saarnio}, {Sanna} and {Silvola}, {Jouko} and {Ikkonen}, {Elena} and {Aaltonen}, {Heikki} and {Nyk}{\"a}nen, {Hannu} and {Martikainen}, {Pertti} {J}.},
  year = {1997},
  keywords = {accumulation,BdGaz,Carbon,CH4,Climate,Microsite,Peatland ecosystem},
  pages = {423--431},
  file = {Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/G99MJEG7/s004420050177.html:;Alm1997:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/SAVEEXMN/Alm1997.pdf:application/pdf}
}

