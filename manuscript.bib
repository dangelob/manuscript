  year = {2011},
  keywords = {CH4,chamber method,fertilized soils,flux measurements,infrared spectroscopy,N2O,peatland,quantum cascade lasers,SPIRIT,water bodies},
  pages = {1--17},
  file = {Guimbaud et al. - 2011 - A portable infrared laser spectrometer for flux me.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/DNC7CWWW/Guimbaud et al. - 2011 - A portable infrared laser spectrometer for flux me.pdf:application/pdf}
