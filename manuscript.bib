  year = {2009},
  keywords = {carbon-climate feedback,carbon content,carbon pools,Climate change,imprim√©,ipcc2013,Peatlands,permafrost soils},
  pages = {n/a--n/a},
  file = {Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/3NZMZDX7/abstract.html:;Tarnocai et al. - 2009 - Soil organic carbon pools in the northern circumpo.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/J2AVBE94/Tarnocai et al. - 2009 - Soil organic carbon pools in the northern circumpo.pdf:application/pdf}
