@article{bond-lamberty2010,
  title = {Temperature-associated increases in the global soil respiration record},
  volume = {464},
  copyright = {{\textcopyright} 2010 {Nature} {Publishing} {Group}},
  issn = {0028-0836},
  doi = {10.1038/nature08930},
  abstract = {Soil respiration, RS, the flux of microbially and plant-respired carbon dioxide (CO2) from the soil surface to the atmosphere, is the second-largest terrestrial carbon flux. However, the dynamics of RS are not well understood and the global flux remains poorly constrained. Ecosystem warming experiments, modelling analyses and fundamental biokinetics all suggest that RS should change with climate. This has been difficult to confirm observationally because of the high spatial variability of RS, inaccessibility of the soil medium and the inability of remote-sensing instruments to measure RS on large scales. Despite these constraints, it may be possible to discern climate-driven changes in regional or global RS values in the extant four-decade record of RS chamber measurements. Here we construct a database of worldwide RS observations matched with high-resolution historical climate data and find a previously unknown temporal trend in the RS record after accounting for mean annual climate, leaf area, nitrogen deposition and changes in CO2 measurement technique. We find that the air temperature anomaly (the deviation from the 1961{\textendash}1990 mean) is significantly and positively correlated with changes in RS. We estimate that the global RS in 2008 (that is, the flux integrated over the Earth's land surface over 2008) was 98 \ensuremath{\pm} 12 Pg C and that it increased by 0.1 Pg C yr-1 between 1989 and 2008, implying a global RS response to air temperature (Q10) of 1.5. An increasing global RS value does not necessarily constitute a positive feedback to the atmosphere, as it could be driven by higher carbon inputs to soil rather than by mobilization of stored older carbon. The available data are, however, consistent with an acceleration of the terrestrial carbon cycle in response to global climate change.},
  language = {en},
  timestamp = {2015-01-13 09:15:50},
  number = {7288},
  urldate = {2015-01-13},
  journal = {Nature},
  author = {Bond-{Lamberty}, {Ben} and {Thomson}, {Allison}},
  month = mar,
  year = {2010},
  pages = {579--582},
  file = {nature08930.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/C24VC6PJ/nature08930.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/D9NJQ8DP/nature08930.html:text/html}
}

@article{francez2000,
  title = {La dynamique du carbone dans les tourbi{\`e}res {\`a} {Sphagnum}, de la sphaine {\`a} l'effet de serre},
  volume = {39},
  doi = {10.1016/S0003-5017(00)90002-3},
  abstract = {Les tourbi{\`e}res {\`a} Sphagnum accumulent de la tourbe et du carbone en fonctionnant comme un syst{\`e}me "puits", la fixation de carbone par la v{\'e}g{\'e}tation {\'e}tant sup{\'e}rieure {\`a} la d{\'e}composition de la mati{\`e}re organique. La production primaire totale annuelle des tourbi{\`e}res sphaignes non bois{\'e}es est en g{\'e}n{\'e}ral inf{\'e}rieure {\`a} 500 g C.m 2, la contribution de la strate muscinale pouvant parfois atteindre pr{\`e}s de 80 \%. L'accumulation annuelle moyenne de carbone dans les tourbi{\`e}res bor{\'e}ales avoisinent 30 g.m -2. L'{\'e}tude des flux entrants et sortants de la tourbi{\`e}re montre que l'accumulation correspond {\`a} 10 \% de la production primaire. Le rapport de l'accumulation h la production augmente du bas- vers le haut-marais et varie avec les esp{\`e}ces v{\'e}g{\'e}tales. Les {\'e}missions de m{\'e}thane des tourbi{\`e}res dans l'atmosph{\`e}re sont {\'e}valu{\'e}es {\`a} 39 Tg.an-1 (1 Tg = 1012 g), soit pr{\`e}s de 35 \% des quantit{\'e}s {\'e}mises par les zones humides. Elles varient avec le type de tourbi{\`e}re, les groupements v{\'e}g{\'e}taux, la saison et peuvent atteindre 3 g CH4.m-2.jour-1. Les tourbi{\`e}res ont beaucoup {\'e}t{\'e} utilis{\'e}es {\`a} des fins agricoles et mini{\`e}res. Le drainage pr{\'e}alable {\`a} ces utilisations et les am{\'e}nagements qui en d{\'e}coulent ont debouch{\'e} sur une remise en circulation des stocks historiquement accumul{\'e}s. Il aurait ainsi {\'e}t{\'e} lib{\'e}r{\'e} 6500 Mt de carbone dans l'atmosph{\`e}re depuis le d{\'e}but du XlXe si{\`e}cle. La contribution des tourbi{\`e}res exploit{\'e}es pour la tourbe comme combustible pourrait {\^e}tre {\`a} terme comparable {\`a} celle du charbon. L ' impact de l'augmentation des d{\'e}p{\^o}ts atmosph{\'e}riques (azote, soufre ...) n'est pas totalement cern{\'e} mais il semble qu'elle puisse avoir entrain{\'e} la disparition de surfaces significatives de tourbi{\`e}res et une modification des communaut{\'e}s (d{\'e}croissance des sphaignes en particulier) dans certaines r{\'e}gions d'Europe. Les exp{\'e}riences de restauration de sites anciennement exploit{\'e}s montrent qu'il est possible, dans certaines conditions, de relancer l'activit{\'e} de tourbification mais la tourbi{\`e}re peut fonctionner comme un syst{\`e}me "source" pour le carbone, plusieurs ann{\'e}es apr{\`e}s les op{\'e}rations de r{\'e}habilitation.},
  timestamp = {2013-05-20 08:45:20},
  journal = {{L}'{Ann}{\'e}e {Biologique}},
  author = {Francez, {Andr}{\'e}-{Jean}},
  year = {2000},
  keywords = {accumulation,acrotelm,bog,catotelm,CH4,CO2,decomposition,disturbance,fen,peat,primary production,restoration,‘sink-source’ ecosystem},
  pages = {205--270},
  file = {Francez2000:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/I4FBMXC2/Francez2000.pdf:application/pdf}
}

