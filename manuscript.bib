  number = {1-3},
  urldate = {2015-07-15},
  journal = {Biogeochemistry},
  author = {Trudeau, {No}{\'e}mie {Cliche} and {Garneau}, {Michelle} and {Pelletier}, {Luc}},
  month = aug,
  year = {2012},
  keywords = {Annual balance,Aqualysis,Biogeosciences,Ecosystems,Environmental Chemistry,fen,Life Sciences; general,Methane,Pools},
  pages = {409--422},
  file = {Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/9UA8HWSU/Trudeau et al. - 2012 - Methane fluxes from a patterned fen of the northea.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/KIP5MF3P/10.html:text/html}
