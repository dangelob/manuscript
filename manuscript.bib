  year = {2006},
  keywords = {BdGaz,carbonBalance,CH4,CO2,ecosystem respiration,imprim√©,net ecosystem exchange,Peat bog,photosynthesis,regeneration,toRead},
  pages = {708--718},
  file = {Bortoluzzi2006a:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/5ITH7W97/Bortoluzzi2006a.pdf:application/pdf}
