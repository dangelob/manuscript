  month = aug,
  year = {1996},
  keywords = {agriculture,Organic soil,Root-derived respiration,Soil respiration,Soil Science \& Conservation},
  pages = {126--131},
  file = {Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/JXMPKSFS/Silvola et al. - 1996 - The contribution of plant roots to CO2 fluxes from.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/X69TU79E/10.html:text/html}
