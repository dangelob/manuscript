  pages = {371--387},
  file = {Full Text PDF:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/ARVDUXHQ/Trudeau et al. - 2014 - Interannual variability in the CO2 balance of a bo.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/HEDZNT8D/10.html:text/html;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/DGCCMIG5/10.html:text/html;trudeau.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/QW2CXRIZ/trudeau.pdf:application/pdf}
