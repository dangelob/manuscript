@article{sonnentag2010,
  title = {On the relationship between water table depth and water vapor and carbon dioxide fluxes in a minerotrophic fen},
  volume = {16},
  copyright = {{\textcopyright} 2009 {Blackwell} {Publishing} {Ltd}},
