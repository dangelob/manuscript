@article{kennedy2005,
  title = {{A} conceptual model of volume-change controls on the hydrology of cutover peats},
  volume = {302},
  issn = {0022-1694},
  doi = {10.1016/j.jhydrol.2004.06.024},
  abstract = {Modeling hydrological processes in certain peats requires a detailed understanding of short-term changes in soil volume and it's influence on the system's hydraulic properties. A study of cutover sites abandoned for 7-years (H92) and 2-years (H97), and an undisturbed section of the Lac Saint-Jean (LSJ) cutover bog was conducted to characterize peat volume changes and its associated hydrological behaviour. Shrinkage and compression accounted for 96{\textendash}97\% of seasonal volume change. Saturated hydraulic conductivity (KS) varied linearly with peat compression; a 1 cm decrease in peat thickness causing a 5.2 cm d-1 decrease in KS at H92, and a 1.4 cm d-1 decrease in KS at H97. A 2{\textendash}6\% seasonal decrease in saturated volumetric moisture content ({\texttheta}S) at the cutover sites was partly due to soil compression, although the development of CH4 bubbles possibly affected moisture content. In the unsaturated zone moisture retentivity ({\texttheta}{\textendash}\ensuremath{\Psi}) was seasonally transient due to the shifting soil structure.

The greater volumetric response to a change in water storage of the more porous and less decomposed peat at H97 indicates a long-term decreasing trend in peat compressibility due to irreversible losses in pore volume. Deterioration of high water storage and low water retention properties at H92 suggests that cutover sites abandoned for longer periods will become increasingly hostile to Sphagnum recolonization despite the blockage of drainage ditches.},
  timestamp = {2015-06-29T08:08:15Z},
  number = {1{\textendash}4},
