  month = feb,
  year = {1992},
  keywords = {Biochemistry; general,carbon dioxide,Geochemistry,microhabitat,Peat,Soil Science \& Conservation,temperature,Terrestrial Pollution,Water table},
  pages = {37--51},
  file = {Kim et Verma - 1992 - Soil surface CO2 flux in a Minnesota peatland.pdf:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/T3I3BQ4P/Kim et Verma - 1992 - Soil surface CO2 flux in a Minnesota peatland.pdf:application/pdf;Snapshot:/home/dangelo/.mozilla/firefox/xy3hpk07.default/zotero/storage/W6M3M8AX/BF00000425.html:text/html}
